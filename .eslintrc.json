{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
      "import/newline-after-import": 0,
      "global-require": 0,
      "consistent-return": 0,
      "max-len": [2, { "code": 100, "ignoreComments": true, "ignoreTrailingComments": true, "ignoreTemplateLiterals": true }],
      "func-names": 0,
      "no-console": 0,
      "no-confusing-arrow": 0,
      // "function-paren-newline": 0
      "comma-dangle": ["error", {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always",
        "exports": "always",
        "functions": "ignore"
      }]
    }
}